HT2	2024.0	7.0	66.0	65.0	0.0#
HT3	2024.0	6.0	51.0	50.0	1.0#
HT4	2024.0	23.0	129.0	123.0	5.0#
"#
#
df <- as.data.frame(read.table(text = cf, sep = " ", header = TRUE))#
#
View(df)#
#
df %>%#
  mutate(`BA Growth` = ifelse(year == 1984, 0,#
    InitialBA[year == 2024] -#
      InitialBA[year == 1984]#
  )) %>%#
  select(Treatments, year, InitialBA, `BA Growth`) %>%#
  melt(id = c("Treatments", "year")) %>%#
  filter(Treatments == "Control" |#
    Treatments == "HT" |#
    Treatments == "HT4") %>%#
  mutate(value = ifelse(year == 2024 & variable == "InitialBA", 0, value))
